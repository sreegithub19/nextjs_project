{
  "routes": [
    {
      "src": "/hello",
      "dest": "/api/hello.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/script/(.*)",
      "dest": "/api/script.js"
    },
    {
      "src": "/go/(.*)",
      "dest": "/api/entrypoint.go"
    }
  ]
}
